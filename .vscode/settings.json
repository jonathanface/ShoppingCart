{
    "editor.codeActionsOnSave": {
        
    },

    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.js$",
                "isAsync": true,
                "cmd": "npm --prefix static/shoppingcart run dev"
            },
            {
                "match": "\\.html$",
                "isAsync": true,
                "cmd": "npm --prefix static/shoppingcart run dev"
            },
            {
                "match": "\\.css$",
                "isAsync": true,
                "cmd": "npm --prefix static/shoppingcart run dev"
            },
        ]
    },
}